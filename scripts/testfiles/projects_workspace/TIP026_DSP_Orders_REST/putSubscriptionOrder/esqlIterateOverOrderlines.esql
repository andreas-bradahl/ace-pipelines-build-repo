BROKER SCHEMA putSubscriptionOrder
CREATE COMPUTE MODULE esqlIterateOverOrderlines
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN		SET Environment.Variables.FailedVariables.OrderLinesFailed = FALSE;		FOR orderLine AS Environment.Variables.Global.OriginRequest.ns:orderLines.ns:putSubscriptionOrderRequestOrderLinesAdd[] DO						SET OutputLocalEnvironment.Variables.CurrentOrderLine = orderLine;			PROPAGATE TO TERMINAL 'out';					END FOR;				FOR orderLine AS Environment.Variables.Global.OriginRequest.ns:orderLines.ns:putSubscriptionOrderRequestOrderLinesUpd[] DO						SET OutputLocalEnvironment.Variables.CurrentOrderLine = orderLine;			PROPAGATE TO TERMINAL 'out1';					END FOR;				FOR orderLine AS Environment.Variables.Global.OriginRequest.ns:orderLines.ns:putSubscriptionOrderRequestOrderLinesDel[] DO						SET OutputLocalEnvironment.Variables.CurrentOrderLine = orderLine;			PROPAGATE TO TERMINAL 'out2';					END FOR;				PROPAGATE TO TERMINAL 'out3';		RETURN FALSE;	END;
END MODULE;
